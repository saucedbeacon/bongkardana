<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:background="@color/color_title_ffffff" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/rl_set_nicknames_top" android:layout_width="130dp" android:layout_height="130dp" android:layout_marginTop="50dp" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0">
        <ImageView android:background="@drawable/adddevice_icon_addsuccess" android:layout_width="90dp" android:layout_height="90dp" android:layout_centerHorizontal="true"/>
        <TextView android:textSize="18sp" android:textColor="@color/ColorBlue" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/str_add_device_completed" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true"/>
    </RelativeLayout>
    <androidx.constraintlayout.widget.ConstraintLayout android:paddingLeft="30dp" android:paddingRight="30dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="50dp" app:layout_constraintTop_toBottomOf="@+id/rl_set_nicknames_top">
        <TextView android:textSize="16sp" android:id="@+id/tv_set_device_nicknames_tips" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/str_set_device_nicknames_tip" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0"/>
        <LinearLayout android:orientation="horizontal" android:id="@+id/ll_set_device_nicknames_btn1" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="15dp" app:layout_constraintTop_toBottomOf="@+id/tv_set_device_nicknames_tips">
            <Button android:textSize="12sp" android:ellipsize="end" android:id="@+id/btn_set_device_nicknames_living_room" android:visibility="visible" android:layout_width="80dp" android:layout_height="35dp" android:layout_margin="5dp" android:text="@string/str_device_nicknames_living_room" android:singleLine="true" android:layout_weight="1" style="@style/RoundedCornerButton"/>
            <TextView android:textSize="12sp" android:gravity="center" android:id="@+id/tv_nicknames_living_room" android:visibility="gone" android:layout_width="80dp" android:layout_height="35dp" android:layout_margin="5dp" android:text="@string/str_device_nicknames_living_room" android:layout_weight="1" style="@style/RoundedCornerButton"/>
            <Button android:textSize="12sp" android:ellipsize="end" android:id="@+id/btn_set_device_nicknames_home" android:layout_width="80dp" android:layout_height="35dp" android:layout_margin="5dp" android:text="@string/str_device_nicknames_home" android:singleLine="true" android:layout_weight="1" style="@style/RoundedCornerButton"/>
            <Button android:textSize="12sp" android:ellipsize="end" android:id="@+id/btn_set_device_nicknames_office" android:layout_width="80dp" android:layout_height="35dp" android:layout_margin="5dp" android:text="@string/str_device_nicknames_office" android:singleLine="true" android:layout_weight="1" style="@style/RoundedCornerButton"/>
            <Button android:textSize="12sp" android:ellipsize="end" android:id="@+id/btn_set_device_nicknames_bedroom" android:layout_width="80dp" android:layout_height="35dp" android:layout_margin="5dp" android:text="@string/str_device_nicknames_bedroom" android:singleLine="true" android:layout_weight="1" style="@style/RoundedCornerButton"/>
        </LinearLayout>
        <LinearLayout android:orientation="horizontal" android:id="@+id/ll_set_device_nicknames_btn2" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/ll_set_device_nicknames_btn1">
            <Button android:textSize="12sp" android:ellipsize="end" android:id="@+id/btn_set_device_nicknames_gate" android:layout_width="80dp" android:layout_height="35dp" android:layout_margin="5dp" android:text="@string/str_device_nicknames_gate" android:singleLine="true" android:layout_weight="1" style="@style/RoundedCornerButton"/>
            <Button android:textSize="12sp" android:ellipsize="end" android:id="@+id/btn_set_device_nicknames_reception" android:layout_width="80dp" android:layout_height="35dp" android:layout_margin="5dp" android:text="@string/str_device_nicknames_reception" android:singleLine="true" android:layout_weight="1" style="@style/RoundedCornerButton"/>
            <Button android:textSize="12sp" android:ellipsize="end" android:id="@+id/btn_set_device_nicknames_kitchen" android:layout_width="80dp" android:layout_height="35dp" android:layout_margin="5dp" android:text="@string/str_device_nicknames_kitchen" android:singleLine="true" android:layout_weight="1" style="@style/RoundedCornerButton"/>
            <Button android:textSize="12sp" android:ellipsize="end" android:id="@+id/btn_set_device_nicknames_study" android:layout_width="80dp" android:layout_height="35dp" android:layout_margin="5dp" android:text="@string/str_device_nicknames_study" android:singleLine="true" android:layout_weight="1" style="@style/RoundedCornerButton"/>
        </LinearLayout>
        <LinearLayout android:background="@drawable/shape_round_corner_edittext_bg" android:layout_width="match_parent" android:layout_height="46dp" android:layout_margin="5dp" app:layout_constraintTop_toBottomOf="@+id/ll_set_device_nicknames_btn2">
            <EditText android:enabled="true" android:textSize="14sp" android:ellipsize="end" android:gravity="left|center_vertical|center_horizontal|center" android:id="@+id/et_set_device_nicknames_custom" android:background="@null" android:paddingLeft="10dp" android:paddingRight="10dp" android:layout_width="match_parent" android:layout_height="match_parent" android:hint="@string/str_set_device_nicknames_custom" android:singleLine="true" android:maxLength="16" app:layout_constraintEnd_toStartOf="0" app:layout_constraintStart_toEndOf="0"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout android:paddingLeft="20dp" android:paddingRight="20dp" android:paddingBottom="20dp" android:layout_width="match_parent" android:layout_height="wrap_content" app:layout_constraintBottom_toBottomOf="0">
        <Button android:id="@+id/btn_set_device_nicknames_completed" android:layout_width="match_parent" android:layout_margin="5dp" android:text="@string/str_finish" app:layout_constraintTop_toTopOf="0" style="@style/RoundedCornerButtonBlueBG"/>
        <Button android:id="@+id/btn_skip_set_device_nicknames" android:layout_width="match_parent" android:layout_marginLeft="5dp" android:layout_marginTop="15dp" android:layout_marginRight="5dp" android:text="@string/str_skip" android:layout_marginHorizontal="5dp" app:layout_constraintTop_toBottomOf="@+id/btn_set_device_nicknames_completed" style="@style/RoundedCornerButton"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
