<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent" android:layout_height="match_parent">
        <com.macrovideo.v380pro.widgets.SwipeToLoadLayoutRecyclerEmptyViewSupport android:id="@+id/swipe_to_load_layout_ucloud_rec_catalog" android:background="@color/ColorBackground" android:layout_width="match_parent" android:layout_height="match_parent">
            <com.macrovideo.v380pro.widgets.AlarmPicHeaderView android:id="@+id/swipe_refresh_header" android:layout_width="match_parent" android:layout_height="wrap_content"/>
            <RelativeLayout android:id="@+id/swipe_target" android:layout_width="match_parent" android:layout_height="match_parent">
                <com.macrovideo.v380pro.widgets.RecyclerViewEmptySupport android:id="@+id/recycler_empty_support_recycler_view" android:layout_width="match_parent" android:layout_height="match_parent" android:splitMotionEvents="false"/>
                <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/cl_ucloud_rec_date_catalog_load_error" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent">
                    <ImageView android:id="@+id/iv_load_error" android:layout_width="131dp" android:layout_height="140dp" android:layout_marginTop="183dp" android:src="@drawable/ucloud_icon_loadingfail" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0"/>
                    <TextView android:textSize="15sp" android:textColor="@color/ColorGrayNormal" android:id="@+id/tv_ucloud_rec_load_error" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="19dp" android:text="@string/str_ucloud_load_fail" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/iv_load_error"/>
                    <Button android:textSize="15sp" android:textColor="@color/ColorBlue" android:gravity="center" android:id="@+id/btn_reload_ucloud_rec" android:background="@drawable/btn_bg_ucloud_selector" android:layout_width="wrap_content" android:layout_height="31dp" android:layout_marginTop="14dp" android:text="@string/str_ucloud_reload" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/tv_ucloud_rec_load_error"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/cl_ucloud_rec_date_catalog_empty_layout" android:visibility="gone" android:layout_width="match_parent" android:layout_height="match_parent">
                    <ImageView android:id="@+id/iv_ucloud_rec_empty" android:layout_width="65dp" android:layout_height="56dp" android:layout_marginTop="183dp" android:src="@drawable/ucloud_icon_novideo" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0"/>
                    <TextView android:textSize="15sp" android:textColor="@color/ColorGrayNormal" android:id="@+id/tv_ucloud_rec_empty" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="19dp" android:text="@string/str_ucloud_no_video" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/iv_ucloud_rec_empty"/>
                    <Button android:textSize="15sp" android:textColor="@color/ColorBlue" android:gravity="center" android:id="@+id/btn_ucloud_rec_empty" android:background="@drawable/btn_bg_ucloud_selector" android:layout_width="wrap_content" android:layout_height="31dp" android:layout_marginTop="14dp" android:text="@string/str_ucloud_reload" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toBottomOf="@+id/tv_ucloud_rec_empty"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </RelativeLayout>
            <com.macrovideo.v380pro.widgets.AlarmPicFooterView android:id="@+id/swipe_load_more_footer" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        </com.macrovideo.v380pro.widgets.SwipeToLoadLayoutRecyclerEmptyViewSupport>
        <androidx.constraintlayout.widget.ConstraintLayout android:id="@+id/cl_expand_tips_layout" android:background="@drawable/shape_gray_radius_13dp" android:paddingLeft="16dp" android:paddingTop="23dp" android:paddingRight="14dp" android:paddingBottom="23dp" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="10dp" android:layout_marginRight="10dp" android:layout_marginBottom="33dp" app:layout_constraintBottom_toBottomOf="0">
            <TextView android:textSize="16sp" android:textColor="@color/color_invariant_ffffff" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_marginRight="10dp" android:text="@string/str_expand_tips" android:includeFontPadding="false" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintLeft_toLeftOf="0" app:layout_constraintRight_toLeftOf="@+id/iv_close_tips" app:layout_constraintTop_toTopOf="0"/>
            <ImageView android:id="@+id/iv_close_tips" android:layout_width="20dp" android:layout_height="20dp" android:src="@drawable/snackbars_btn_close" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintRight_toRightOf="0" app:layout_constraintTop_toTopOf="0"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>
