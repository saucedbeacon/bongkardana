<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <TableRow android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="match_parent">
            <ImageView android:layout_gravity="center" android:id="@+id/iv_date_dot" android:layout_width="13dp" android:layout_height="13dp" android:layout_marginLeft="3dp" android:layout_marginTop="1dp" app:srcCompat="@drawable/my_album_icon_dot"/>
            <TextView android:textSize="16sp" android:textColor="@color/color_666666" android:layout_gravity="center" android:id="@+id/tv_date" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="6dp" android:text="yyyy-mm-dd" android:layout_weight="1"/>
            <ImageView android:layout_gravity="center" android:id="@+id/iv_date_arrow" android:layout_width="15dp" android:layout_height="15dp" android:layout_marginRight="3dp" app:srcCompat="@drawable/devicelist_btn_next"/>
        </LinearLayout>
    </TableRow>
</LinearLayout>
